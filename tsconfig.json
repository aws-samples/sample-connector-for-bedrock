{
  "compilerOptions": {
    "target": "es2020",
    "module": "NodeNext",
    "types": [
      "node"
    ],
    "moduleResolution": "NodeNext",
    "strict": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "rootDir": "./src",
    "outDir": "./dist/server",
  },
  "sourceMap": true,
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
